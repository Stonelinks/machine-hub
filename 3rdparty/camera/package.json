{
  "name": "Camera",
  "version": "1.0.0",
  "main": "",
  "repository": "git@github.com:Stonelinks/Camera.git",
  "author": "Lucas Doyle <lucas.p.doyle@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "12.15.0",
    "yarn": "1.22.4"
  },
  "private": true,
  "scripts": {
    "build": "fix-lint && cd camserver-viewer && yarn build",
    "clean": "rm -rf yarn.lock node_modules && cd camserver-viewer && rm -rf yarn.lock node_modules build",
    "dev-client": "cd camserver-viewer && yarn dev-start",
    "dev-server": "ts-node-dev --no-notify src/server.ts",
    "dev": "yarn sync-common && concurrently \"yarn dev-server\" \"yarn dev-client\"",
    "check-types": "tsc --noEmit src/server.ts",
    "fix": "fix-lint",
    "lint": "fix-lint",
    "reset": "yarn clean && yarn setup",
    "scripts": "cat package.json | jq .scripts",
    "setup": "yarn && yarn sync-common && cd camserver-viewer && yarn",
    "sync-common": "sync-common",
    "start-server": "ts-node -T src/server.ts",
    "start": "yarn setup && yarn sync-common && yarn build && concurrently \"yarn start-server\" \"python -m webbrowser http://localhost:4001\""
  },
  "prettier": {
    "trailingComma": "all"
  },
  "dependencies": {
    "@swc/core": "^1.2.93",
    "@swc/helpers": "^0.2.13",
    "@types/express": "^4.17.6",
    "@types/express-ws": "^3.0.0",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/luxon": "^1.27.0",
    "@types/pako": "^1.0.2",
    "@types/request": "^2.48.7",
    "@types/ws": "^8.2.0",
    "bufferutil": "^4.0.4",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "del": "^6.0.0",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "ffmpeg-static": "^4.2.7",
    "flat-cache": "^2.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "i": "^0.3.6",
    "luxon": "^1.27.0",
    "node-fetch": "^2.6.1",
    "npm": "^7.20.3",
    "pako": "^2.0.4",
    "prettier": "^1.19.1",
    "promisify-child-process": "^3.1.3",
    "react-icons": "^3.10.0",
    "request": "^2.88.2",
    "rtsp-streaming-server": "^2.0.6",
    "sharp": "^0.25.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.0.0",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^4.1.2",
    "utf-8-validate": "^5.0.6",
    "uuid-by-string": "^3.0.4",
    "v4l2camera": "Williangalvani/node-v4l2camera",
    "ws": "^8.2.3"
  }
}
